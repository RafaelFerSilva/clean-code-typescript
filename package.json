    "@shelf/jest-mongodb": "^4.1.7",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.15",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "git-commit-msg-linter": "^4.9.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "rimraf": "^5.0.1",
